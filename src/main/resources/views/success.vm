<html>
<head>
    <title>Success</title>
</head>
<body>
<div class="form-body">
    <h3 class="formtitle">Success</h3>

    <div>
        #foreach($message in $messages)
            <p>$message</p>
        #end
    </div>
</div>
<script type="text/javascript">
        #foreach($message in $messages)
        JIRA.Messages.showMsg("$message", {
            type: JIRA.Messages.Types.SUCCESS,
            closeable: true
        });
        #end
    JIRA.Dialog.current.hide();
</script>
</body>
</html>